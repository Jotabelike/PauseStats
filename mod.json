{
	"geode": "4.9.0",
	"gd": {
		"win": "2.2074",
		"android": "2.2074",
		"mac": "2.2074",
		"ios": "2.2074"
	},
	"id": "jotabelike.pausestats",
	"name": "Pause Stats",
	"version": "v1.3.0",
	"developer": "XJotabelikeX",
	"description": "Simple and accessible information interface!",
	"tags": [
		"interface",
		"utility",
		"offline"
	],
	"dependencies": {
		"cvolton.level-id-api": ">=v1.0.1"
	},
	"settings": {
		"show_attempts": {
			"type": "bool",
			"default": true,
			"name": "Show Attempts"
		},
		"show_jumps": {
			"type": "bool",
			"default": true,
			"name": "Show Jumps"
		},
		"show_completions": {
			"type": "bool",
			"default": true,
			"name": "Show Completions"
		},
		"show_object_count": {
			"type": "bool",
			"default": true,
			"name": "Show Object Count",
			"info": "El conteo de objetos puede ser inexacto en niveles muy grandes o complejos."
		},
		"show_checkpoints": {
			"type": "bool",
			"default": true,
			"name": "Show Checkpoints",
			"info": "Muestra cuántos checkpoints tiene el nivel (solo en modo plataforma)."
		},
		"show_next_key": {
			"type": "bool",
			"default": true,
			"name": "Show Next Key Progress",
			"info": "Muestra cuántos orbes faltan para la siguiente llave (de 500)."
		},
		"show_device": {
			"type": "bool",
			"default": true,
			"name": "Show Device"
		},
		"show_gamemode": {
			"type": "bool",
			"default": true,
			"name": "Show Gamemode"
		},
		"show_level_id": {
			"type": "bool",
			"default": true,
			"name": "Show Level ID"
		},
		"show_song_id": {
			"type": "bool",
			"default": true,
			"name": "Show Song ID"
		},
		"stats_horizontal": {
			"type": "bool",
			"default": false,
			"name": "Horizontal Layout"
		},
		"stats_x": {
			"type": "float",
			"default": 42,
			"min": 0,
			"max": 800,
			"name": "X Position",
			"control": {
				"slider": true
			}
		},
		"stats_y": {
			"type": "float",
			"default": 290,
			"min": 0,
			"max": 600,
			"name": "Y Position",
			"control": {
				"slider": true
			}
		},
		"stats_spacing": {
			"type": "float",
			"default": 31,
			"min": 10,
			"max": 80,
			"name": "Stats Spacing",
			"control": {
				"slider": true
			}
		},
		"stats_scale": {
			"type": "float",
			"default": 0.7,
			"min": 0.5,
			"max": 2,
			"name": "Stats Size",
			"control": {
				"slider": true
			}
		},
		"text_opacity": {
			"type": "float",
			"default": 1,
			"min": 0.1,
			"max": 1,
			"name": "Text Opacity",
			"control": {
				"slider": true
			}
		}
	}
}